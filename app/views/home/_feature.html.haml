    %ul#myTab.nav.nav-tabs{:role => "tablist"}
      %li.active{:role => "presentation"}
        %a#expeditions-tab{"aria-controls" => "expeditions", "aria-expanded" => "true", "data-toggle" => "tab", :href => "#expeditions", :role => "tab"} UpComing cars
      %li{:role => "presentation"}
        %a#tours-tab{"aria-controls" => "tours", "data-toggle" => "tab", :href => "#tours", :role => "tab"} Popular
      %li{:role => "presentation"}
        %a#tree-tab{"aria-controls" => "tree", "data-toggle" => "tab", :href => "#tree", :role => "tab"} Just Launched
