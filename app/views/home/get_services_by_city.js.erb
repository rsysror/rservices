$('.loading').hide();
<% if @services.present? %>
  $('#service-title').html('');
  $('#services_data').html('');
  $('#services_data').html("<%= escape_javascript render(:partial => 'services_list', :locals => { :services => @services }) %>");
<% else %>
  $('#services_data').html('');
  $('#service-title').html("No service is available. Please select another city and proceed further.");
<% end %>