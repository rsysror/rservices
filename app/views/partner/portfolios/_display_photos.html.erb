<% portfolio.images.each_with_index do |image, index| %>
  <div class="gallery">
    <%= image_tag("#{image.url}", alt: "IMG-PRODUCT") %>
    <div class="portfolio_delete_image">
      <%= link_to ("<i class='fa fa-trash'></i>").html_safe, delete_photo_partner_portfolio_path(id: portfolio.id, index: index), :method => :delete, data: { confirm: "Are you sure, you want to delete this image?" } %>
    </div>
  </div>
<% end %>
