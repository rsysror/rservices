<tr>
	<td><%=request.service.try(:name) %></td>
	<td><%=request.address.flat_number %>,<%=request.address.street_name %>,<%=request.address.pin_code %></td>
	<td class='<%=(request.status.try(:name) == "Cancelled") ?  'alert alert-danger' : '' %>'><b><%=request.status.try(:name) %></b></td>
	<td><%=request.portfolio.try(:portfolio_with_user_name_and_about) %></td>
	<td><%=request.try(:time_slot).try(:start_time_with_end_time) %></td>
	<td><%=request.service_request_number %></td>
	<td>  
		<%= link_to t('.show', :default => t("helpers.links.show")),service_request_path(request), :class => 'btn btn-default btn-xs' %>
    <%= link_to t('.edit', :default => t("helpers.links.edit")),edit_service_request_path(request), :class => 'btn btn-default btn-xs' %>
    <%= link_to t('.cancel', :default => t("helpers.links.cancel")),cancel_service_request_service_request_path(request),:method => :patch,:data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },:class => 'btn btn-xs btn-danger' %>
  </td>  
</tr>