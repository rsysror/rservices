<div class="main-container">
	<div class="row">
		<%= link_to t('.Back', :default => t("helpers.links.back")),dashboard_path,:class => 'btn btn-primary' %>
		<table class="table">
			<thead>
				<th>Service Request Name</th>
				<th>Address</th>
				<th>Status</th>
				<th>Vendor</th>
				<th>Time Slot</th>
				<th>Service Request Number</th>
				<th>Actions</th>
			</thead>
			<tbody>
				<% @service_requests.each do |request|%>
					<%= render :partial => 'service_request_list', :locals => { :request => request } %>
				<% end %>
			</tbody>
		</table>
	</div>
	<div class="pagination">
			<%=will_paginate @service_requests %>
	</div>
</div>

