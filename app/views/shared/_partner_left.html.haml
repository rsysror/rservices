.navbar-default.sidebar
  %ul.side-menu.nav.sidebar-color
    %li
      %a{:href => "/partner/portfolio"}
        %i.fa.fa-picture-o
        Portfolio
    %li
      %a{:href => "/partner/dashboard"}
        %i.fa.fa-dashboard.fa-fw
        Dashboard
    %li#lastli
      %a{:href => "/partner/portfolio/edit"}
        %i.fa.fa-pencil-square-o
        Edit Profile

:javascript
  $('.side-menu a').filter(function(){
    return this.href==location.href}).addClass('active').siblings('a').removeClass('active')
    $('.side-menu a').click(function(){
    $(this).addClass('active').siblings().removeClass('active')  
  })
